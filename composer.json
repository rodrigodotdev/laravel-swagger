{
    "name": "rayo/laravel-swagger",
    "description": "A simple swagger integration with laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "swagger-api/swagger-ui": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0"
    },
    "authors": [
        {
            "name": "Rodrigo C.",
            "email": "rodrigo.versatec@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rayo\\LaravelSwagger\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rayo\\LaravelSwagger\\SwaggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
